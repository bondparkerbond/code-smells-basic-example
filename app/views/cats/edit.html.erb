<h1>Editing Cat</h1>

<%= form_for [@owner, @cat] do |f| %>

  <%= f.label :owner_id %>
  <%= f.text_field :owner_id%>

  <%= f.label :name %>
  <%= f.text_field :name%>

  <%= f.label :age %>
  <%= f.text_field :age%>

  <%= f.label :fur_color %>
  <%= f.text_field :fur_color %>

  <%= f.label :eye_color %>
  <%= f.text_field :eye_color %>

  <%= f.label :food_type %>
  <%= f.text_field :food_type %>

  <%= f.submit %>

<% end %>

<%= link_to 'Show', owner_cat_path %> |
<%= link_to 'Back', owner_path(@cat.owner_id) %>
