<h1>Listing All Owners Cats</h1>

<% @cats.each do |cat| %>
  
  <br><%= cat.name %>
  <br><%= link_to 'Show', cat_path(cat) %> |
  <%= link_to 'Edit', edit_cat_path(cat) %> |  
  <%= link_to 'Destroy', cat, method: :delete, data: { confirm: 'Are you sure?' } %>
  <br>
<% end %>

<br><%= link_to 'New Cat', new_cat_path %>
